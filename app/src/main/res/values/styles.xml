<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="MyCustomThemeToolbar" parent="ThemeOverlay.AppCompat.Dark">
        <item name="android:textColorPrimary">#d1d6ef</item>
        <item name="android:textColorSecondary">#ffffff</item>
    </style>

    <style name="MyCustomOverlay" parent="ThemeOverlay.AppCompat.Dark">
        <item name="android:background">#feffff</item>
        <item name="android:textColorPrimary">#4d4d4d</item>
    </style>
</resources>
